<UserControl x:Class="DatabaseController.CommandExecutors.View.SellCommandExecutorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DatabaseController.CommandExecutors.View"
             xmlns:resx="clr-namespace:DatabaseController.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="380" d:DesignWidth="300">
    <StackPanel Orientation="Vertical" Background="#FFDDCEB0">
        <Label Content="{Binding Source={x:Static resx:Resources.whichoneLabel}}" Margin="30,40,10,0"/>
        <TextBox Text="{Binding Which}" Margin="10,-30,0,0" ></TextBox>
        <Label Content="{Binding Source={x:Static resx:Resources.howmanyLabel}}" Margin="50,20,10,0"/>
        <TextBox Text="{Binding HowMany}" Margin="10,-30,0,0"></TextBox>
    </StackPanel>
</UserControl>
